{
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 20,
  "timezone": "America/New_York",
  "schedule": ["every 3 months"],
  "packageRules": [
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "matchPackagePatterns": [
        ".*"
      ],
      "automerge": true,
      "automergeType": "branch"
    }
  ]
}
