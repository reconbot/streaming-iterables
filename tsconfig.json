{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": false,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2020",
    "useUnknownInCatchVariables": false,
  },
  "include": [
    "lib/*",
  ],
  "exclude": [
    "node_modules"
  ]
}
