    "noEmit": true,
    "allowJs": false,
    "target": "ES2018",
    "moduleResolution": "node",
    "module": "commonjs",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "declaration": true,
    "importHelpers": true
  },
  "include": [
    "lib/*",
  ],
  "exclude": [
    "node_modules"
  ]
}
